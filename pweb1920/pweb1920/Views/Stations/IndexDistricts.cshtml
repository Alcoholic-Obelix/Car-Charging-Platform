@model IEnumerable<string>

@{
    ViewBag.Title = "IndexDistricts";
}

<h2>IndexDistricts</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Districts
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.ActionLink(item.ToString(), "IndexCities", "Stations", new { district = item.ToString() }, null) 
        
    </td>
</tr>
}

</table>
