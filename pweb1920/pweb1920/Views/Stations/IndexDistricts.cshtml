@model pweb1920.Models.DTO.IndexStationDTO

@{
    ViewBag.Title = "Districts";
}

<h2>Districts</h2>

<table class="table">
    <tr>
        <th>
            District
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Stations) {
<tr>
    <td>
        @Html.ActionLink(item.District.ToString(), "Index", "Stations", new { district = item.Id }, null) 
        
    </td>
</tr>
}

</table>
