@model pweb1920.Models.DTO.IndexStationDTO

<h2>Stations</h2>
<h4>@ViewBag.Title</h4>

<table class="table">
    <tr>
        <th>
            District
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Stations) {
<tr>
    <td>
        @Html.ActionLink(item.District.ToString(), "Search", "Stations", new { district = item.Id }, null) 
        
    </td>
</tr>
}

</table>
