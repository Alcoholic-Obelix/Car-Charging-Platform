@model IEnumerable<string>

<h2>Cities</h2>

<table class="table">
    <tr>
        <th>
            City
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.ToString(), "IndexCities", item.ToString())

            </td>
        </tr>
    }

</table>