@model IEnumerable<string>

@{
    ViewBag.Title = "IndexCities";
}

<h2>IndexCities</h2>

<table class="table">
    <tr>
        <th>
            City
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.ToString(), "IndexStations", "Stations", new { city = item.ToString() }, null)

            </td>
        </tr>
    }

</table>